@model LoginProjectDomain.Models.HostInfo;
@{
    ViewData["Title"] = "Add";
}

<h1>Add</h1>




<form asp-action="Add" asp-controller="Home" method="post">
    <div class="form-group">
        <label asp-for="Id"></label>
        <input asp-for="Id" class="form-control"/>
        <span asp-validation-for="Id" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Name"></label>
        <input asp-for="Name" class="form-control"/>
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="BandWidth"></label>
        <input asp-for="BandWidth" class="form-control" />
        <span asp-validation-for="BandWidth" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="OnlineSpace"></label>
        <input asp-for="OnlineSpace" class="form-control" />
        <span asp-validation-for="OnlineSpace" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Price"></label>
        <input asp-for="Price" class="form-control" />
        <span asp-validation-for="Price" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Support"></label>
        <input type="checkbox" class="form-check-input" asp-for="Support" class="form-control" />
    </div>

    <button type="submit" class="btn btn-primary">Add</button>


</form>
